<% if signed_in? %>
  <div class="center jumbotron">
    <h1>Hello <%= current_user.name %></h1>
    <h2>This is your home page</h2>
  </div>
<% else %>
  <div class="center jumbotron">
    <h1>Welcome to MYINSTA</h1>
    <%= link_to "Sign up now!", new_user_registration_path, class: "btn btn-lg btn-primary" %>
  </div>
<% end %>
