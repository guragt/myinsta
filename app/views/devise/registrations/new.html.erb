<h2>Sign up</h2>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= simple_form_for @user, url: registration_path(@user) do |f| %>
      <%= f.input :name %>
      <%= f.input :nickname %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.input :password_confirmation %>
      <%= f.button :submit, "Sign up", class: "btn btn-primary" %>
    <% end %>
    <%= render "devise/shared/links" %>
  </div>
</div>

